{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"standard"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module",
		"requireConfigFile": false
	},
	"rules": {
		"import/no-webpack-loader-syntax": "off"
	},
	"ignorePatterns": ["*.json"]
}
